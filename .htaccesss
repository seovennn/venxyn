<IfModule mod_rewrite.c>
    RewriteEngine On

    # Redirect jika file/folder tidak ditemukan (404)
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteRule ^(.*)$ redirect404.php [L]

    # Arahkan semua request ke folder public (aturan asli kamu)
    RewriteRule ^(.*)$ public/$1 [L]
</IfModule>

